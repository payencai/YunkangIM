<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StickNavLayout">
        <!--默认是否悬停tab-->
        <attr name="isStickNav" format="boolean" />
        <!--距离悬浮的位置-->
        <attr name="stickOffset" format="dimension" />
    </declare-styleable>
    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
    <!--自定义bottombar 属性-->
    <attr name="arcHeight" format="dimension"/>
    <declare-styleable name="CircleImageView">
        <attr name="my_radius" format="integer" />
    </declare-styleable>
    <declare-styleable name="ArcView">
        <attr name="arcHeight"/>
        <attr name="bgColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ArcImageView">
        <attr name="arcHeight"/>
    </declare-styleable>
    <!--自定义dialog背景全透明无边框theme -->



</resources>